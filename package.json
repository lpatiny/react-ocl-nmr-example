{
  "name": "react-ocl-nmr",
  "version": "1.1.1",
  "description": "A component to display chemical structure for NMR spectra assignments",
  "main": "./lib/OCLnmr.js",
  "module": "./lib-esm/OCLnmr.js",
  "files": [
    "src",
    "lib",
    "lib-esm"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakodium/react-ocl-nmr.git"
  },
  "keywords": [],
  "author": "Luc Patiny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakodium/react-ocl-nmr/issues"
  },
  "peerDependencies": {
    "openchemlib": "*"
  },
  "dependencies": {
    "openchemlib-utils": "^1.2.0",
    "react-ocl": "^4.3.3"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "clean": "rimraf lib lib-esm",
    "dev": "vite",
    "eslint": "eslint src --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepublishOnly": "npm run tsc",
    "test": "npm run eslint && npm run check-types",
    "test-coverage": "npm run test-only -- --coverage",
    "test-only": "jest",
    "tsc": "npm run clean && npm run tsc-cjs && npm run tsc-esm",
    "tsc-cjs": "tsc --project tsconfig.cjs.json",
    "tsc-esm": "tsc --project tsconfig.esm.json"
  },
  "devDependencies": {
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@vitejs/plugin-react-refresh": "^1.3.3",
    "@zakodium/eslint-config": "^3.0.2",
    "eslint": "^7.29.0",
    "openchemlib": "^7.2.3",
    "postcss": "^8.3.5",
    "prettier": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.2.2",
    "typescript": "^4.3.4",
    "vite": "^2.3.8"
  }
}
